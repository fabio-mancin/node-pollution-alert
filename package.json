{
  "name": "node-pollution-alert",
  "version": "0.0.1",
  "description": "Pollution alert NodeJS app.",
  "scripts": {
    "setup": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "build:prod": "webpack --mode=production",
    "dev": "npm-run-all --parallel watch:server watch:build",
    "watch:server": "nodemon app.js",
    "watch:build": "webpack --watch --mode=development",
    "prod": "cross-env NODE_ENV=production && npm run build:prod && node app.js",
    "test": "mocha tests/**/*.js --exit --timeout 10000"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "fs-extra": "^7.0.1",
    "leaflet": "^1.7.1",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.88.0",
    "sequelize": "^6.6.2",
    "supertest": "^6.1.3",
    "svg-url-loader": "^7.1.1",
    "tooltip": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-pug-plugin": "^3.0.0",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pug-loader": "^2.4.0",
    "sass-loader": "^11.0.1",
    "sequelize-cli": "^6.2.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
